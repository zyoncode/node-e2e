{
  "name": "@node-e2e/cli",
  "version": "1.0.0",
  "description": "A unified End-to-End testing framework for React Native and Web projects.",
  "main": "api/index.js",
  "type": "module",
  "bin": "./cli/index.js",
  "scripts": {
    "touch": "./cli/index.js test --platform touch --framework wdio",
    "ios": "./cli/index.js test --platform ios --framework wdio",
    "android": "./cli/index.js test --platform android --framework wdio",
    "web": "./cli/index.js test --platform web --framework wdio",
    "electron": "./cli/index.js test --platform electron --framework wdio",
    "ext": "./cli/index.js test --platform ext --framework wdio",
    "debug:android": "./cli/index.js debug --platform android --framework wdio",
    "wdio": "wdio run ./wdio.conf.js"
  },
  "keywords": [
    "e2e",
    "testing",
    "framework",
    "react-native",
    "web"
  ],
  "author": "joe",
  "license": "MIT",
  "devDependencies": {
    "@types/mocha": "^10",
    "@types/yargs": "^17",
    "@wdio/allure-reporter": "^8.31.1",
    "@wdio/appium-service": "^8.32.1",
    "@wdio/cli": "^8.32.1",
    "@wdio/devtools-service": "^8.32.1",
    "@wdio/local-runner": "^8.32.1",
    "@wdio/mocha-framework": "^8.31.1",
    "@wdio/spec-reporter": "^8.31.0",
    "allure-commandline": "^2.24.1",
    "appium-uiautomator2-driver": "^3.1.0",
    "appium-xcuitest-driver": "^7.1.0",
    "dotenv": "^16.3.1",
    "eslint": "^8.52.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "framer-motion": "^10.16.5",
    "glob": "^10.3.4",
    "mocha": "^10.2.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4",
    "wdio-electron-service": "^6.3.0",
    "wdio-performancetotal-service": "^3.1.0",
    "yargs": "^17.7.2"
  },
  "dependencies": {
    "@appium/support": "^4.1.10",
    "@wdio/shared-store-service": "^8.32.4",
    "electron": "^27.0.3",
    "lodash-es": "^4.17.21",
    "tesseract.js": "^5.0.4",
    "web-worker": "^1.2.0",
    "webdriver": "^8.32.0",
    "webdriverio": "^8.32.1",
    "ws": "^8.15.1"
  }
}
